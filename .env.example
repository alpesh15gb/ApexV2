SQLEXPRESS_TABLE=punch_logs
SQLEXPRESS_COL_EMPID=employee_id
SQLEXPRESS_COL_PUNCHTIME=punch_time
SQLEXPRESS_COL_PUNCHTYPE=punch_type

# MySQL Hikvision DB
